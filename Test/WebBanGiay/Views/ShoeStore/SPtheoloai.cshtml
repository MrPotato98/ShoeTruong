@model IEnumerable<WebBanGiay.Models.GIAY>
@{
    ViewBag.Title = "SPtheoloai";
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
}

<h2>SPtheoloai</h2>
<div class="row">
    @foreach (var x in Model)
    {
        <a href="#">
            <div class="col-md-4 product-men">
                <div class="product-shoe-info shoe">
                    <div class="men-pro-item">
                        <div class="men-thumb-item">
                            <img src="~/image/@Url.Content(@x.Anhbia)" style="height:200px;width:100%" alt="">
                            <div class="men-cart-pro">
                                <div class="inner-men-cart-pro">
                                    <a href="@Url.Action("Details", "ShoeStore", new { id = x.MaGIAY })" class="link-product-add-cart">Quick View</a>
                                </div>
                            </div>
                            <span class="product-new-top">New</span>
                        </div>
                        <div class="item-info-product">
                            <h4>
                                @x.TenGIAY
                            </h4>
                            <div class="info-product-price">
                                <div class="grid_meta">
                                    <div class="product_price">
                                        <div class="grid-price ">
                                            <span class="money ">@x.Giaban</span>
                                        </div>
                                    </div>

                                </div>

                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div>
                </div>
            </div>
        </a>
    }
</div>
   